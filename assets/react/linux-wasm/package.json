{
  "name": "linux-wasm",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@react-hookz/web": "^14.2.2",
    "@types/react-dom": "^18.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "v86": "^0.3.6",
    "xterm": "^4.18.0"
  },
  "scripts": {
    "build": "esbuild ./src/index.tsx --inject:src/dbg-shim.ts --bundle --minify --outfile=./build/bundle.js",
    "build-watch": "esbuild ./src/index.tsx --inject:src/dbg-shim.ts --bundle --minify --watch --outfile=./build/bundle.js",
    "build-dev": "esbuild ./src/index.tsx --inject:src/dbg-shim.ts --bundle --sourcemap=external --outfile=./build/bundle.js",
    "build-dev-watch": "esbuild ./src/index.tsx --inject:src/dbg-shim.ts --bundle --sourcemap=external --watch --outfile=./build/bundle.js"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "esbuild": "^0.14.5",
    "eslint": "^8.4.1",
    "typescript": "^4.5.2"
  }
}
